<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE aspectran PUBLIC "-//ASPECTRAN//DTD Aspectran 9.0//EN"
        "https://aspectran.com/dtd/aspectran-9.dtd">
<aspectran>

    <translet name="/examples/hello-world">
        <echo id="page">
            <item name="headline">Hello, World!</item>
            <item name="teaser">This is the output of a JSP page that is part of the HelloWorld application.</item>
            <item name="include">examples/hello-world</item>
            <item name="style">plate</item>
        </echo>
        <dispatch name="templates/default"/>
    </translet>

</aspectran>
