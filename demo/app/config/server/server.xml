<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE aspectran PUBLIC "-//ASPECTRAN//DTD Aspectran 8.0//EN"
        "http://aspectran.github.io/dtd/aspectran-8.dtd">
<aspectran>

    <description>
        This demo app uses the Undertow server by default.
        To use the Jetty server, activate the "jetty" profile.
    </description>

    <append file="/config/server/undertow/tow-server.xml"/>
    <append file="/config/server/undertow/tow-context.xml"/>
    <append file="/config/server/jetty/jetty-server.xml"/>
    <append file="/config/server/jetty/jetty-context.xml"/>
    <append resource="com/aspectran/aspectow/appmon/context/tow-context-appmon.xml"/>
    <append file="/config/server/undertow/tow-support.xml"/>

</aspectran>
