<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE aspectran PUBLIC "-//ASPECTRAN//DTD Aspectran 8.0//EN"
        "http://aspectran.github.io/dtd/aspectran-8.dtd">
<aspectran>

    <bean id="nestedInnerStringBean" class="java.lang.String" scope="prototype">
        <arguments>
            <item>
                <bean class="java.lang.String">
                    <arguments>
                        <item>
                            <bean class="java.lang.String">
                                <arguments>
                                    <item>
                                        <bean class="java.lang.String">
                                            <arguments>
                                                <item>Nested Inner String Bean</item>
                                            </arguments>
                                        </bean>
                                    </item>
                                </arguments>
                            </bean>
                        </item>
                    </arguments>
                </bean>
            </item>
        </arguments>
    </bean>

</aspectran>